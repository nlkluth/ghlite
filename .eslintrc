{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": [1, "never"],
    "space-before-function-paren": [2, "never"],
    "arrow-spacing": [2, {"before": true, "after": true}],
    "react/jsx-boolean-value": [2, "always"],
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-param-reassign": 0
  }
}
